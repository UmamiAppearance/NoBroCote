{
  "name": "no-bro-cote",
  "version": "0.1.0",
  "description": "Node Browser Code Tests. Run tests on your JavaScript code for the browser.",
  "main": "./cjs/no-bro-cote.cjs",
  "module": "./src/no-bro-cote.js",
  "type": "module",
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src/",
    "build-cjs": "rollup ./src/no-bro-cote.js --file ./cjs/no-bro-cote.cjs --format cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UmamiAppearance/NoBroCote.git"
  },
  "keywords": [
    "Test",
    "Test Runner",
    "Browser",
    "Puppeteer"
  ],
  "author": "UmamiAppearance",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/UmamiAppearance/NoBroCote/issues"
  },
  "homepage": "https://github.com/UmamiAppearance/NoBroCote#readme",
  "dependencies": {
    "puppeteer": "^14.1.0"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "rollup": "^2.73.0"
  }
}
